---
import BaseLayout from "../layouts/BaseLayout.astro";

import ContactCTA from "../components/ContactCTA.astro";
import Hero from "../components/Hero.astro";
import InnerPageBanner from "../components/InnerPageBanner.astro";
---

<BaseLayout title="Donate" description="About Jeanine White Lorem Ipsum">
	<div class="stack">
		<main class="">
			<div class="banner-top">
				<div class="wrapper">
					<InnerPageBanner
						title="Donate"
						pageName="donate"
						image="/public/assets/donate.png"
					/>
				</div>
			</div>
		</main>
	</div>
</BaseLayout>

<style>
	.banner-top {
		padding: 100px 0;
		background-color: var(--gray-900);
	}
</style>
